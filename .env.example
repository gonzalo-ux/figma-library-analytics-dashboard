# Figma API Configuration
# Get your access token from: https://www.figma.com/settings
VITE_FIGMA_ACCESS_TOKEN=your_figma_access_token_here

# Figma Library URL
# Example: https://www.figma.com/file/ABC123XYZ/Library-Name
VITE_FIGMA_LIBRARY_URL=

# Python API Configuration (for CSV generation)
# Path to the Python figma-analytics-api directory
# Leave empty if using default location
PYTHON_API_PATH=

# Changelog Configuration
# For Google Docs integration
GOOGLE_DOCS_ID=
GOOGLE_DOCS_API_KEY=

# For Notion integration
NOTION_API_KEY=
NOTION_DATABASE_ID=
